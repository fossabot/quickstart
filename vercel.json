{
  "version": 2,
  "rewrites": [
    {
      "source": "/service-worker.js",
      "destination": "/public/service-worker.js"
    },
    {
      "source": "/workbox-service-worker.js",
      "destination": "/public/workbox-service-worker.js"
    }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/workbox-service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "redirects": [
    {
      "source": "/storybook",
      "destination": "/storybook/index.html"
    }
  ]
}