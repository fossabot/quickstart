{
  "name": "quickstart",
  "author": "@piprees",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "predev": "yarn install",
    "dev": "yarn concurrently -n next,storybook,jest -c green,magenta,cyan --kill-others --restart-tries 0  --handle-input --default-input-target 2 \"yarn next dev\" \"yarn storybook\" \"yarn test:unit:json:watch\"",
    "prebuild": "yarn storybook:build",
    "build": "next build",
    "start": "yarn next start",
    "lint": "yarn lint:css && yarn lint:md && yarn lint:js && yarn lint:types",
    "lint:css": "yarn stylelint \"./**/*.css\" --fix",
    "lint:js": "yarn eslint --fix --ext ts --ext tsx --ext js --ext jsx --ext json .",
    "lint:types": "yarn tsc --pretty --noEmit",
    "lint:md": "markdownlint \"./**/*.md\" --ignore node_modules --fix",
    "test": "yarn test:unit && yarn commit:await:local",
    "test:unit": "yarn jest",
    "test:unit:watch": "yarn test:unit --watch",
    "test:unit:json": "jest --json --outputFile=.storybook/.jest-test-results.json || true",
    "test:unit:json:watch": "jest --json --outputFile=.storybook/.jest-test-results.json --watch  || true",
    "test:integration": "yarn cypress run",
    "test:integration:watch": "yarn cypress open",
    "commit": "yarn lint && yarn test:unit && yarn commit:await:local",
    "commit:await:local": "yarn start-server-and-test \"yarn next dev\" http://localhost:3000 \"yarn commit:await:storybook\"",
    "commit:await:storybook": "yarn start-server-and-test \"yarn storybook\" http://localhost:6006 \"yarn test:integration\"",
    "clean:storybook": "rm -rf ./public/storybook || true",
    "storybook": "yarn start-storybook -p 6006 --loglevel warn --quiet",
    "prestorybook:build": "yarn clean:storybook && yarn test:unit:json",
    "storybook:build": "yarn build-storybook --loglevel warn --quiet --output-dir ./public/storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn commit"
    }
  },
  "browserslist": "> 0.5% in my stats",
